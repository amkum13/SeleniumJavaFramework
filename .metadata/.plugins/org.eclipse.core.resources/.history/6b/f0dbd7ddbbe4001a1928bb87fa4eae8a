	static String projectPath = System.getProperty("user.dir");

	public static void getRowCount() {
		
		try {
			XSSFWorkbook workbook = new XSSFWorkbook(projectPath+"\\excel\\data.xlsx");
			
		} catch (IOException e) {
			
			e.printStackTrace();
		}
	}

}
