package ignoreTests;

import org.testng.annotations.Ignore;
import org.testng.annotations.Test;

@Ignore
public class IgnoreTestsDemo {
	
	@Ignore
	@Test()
	public void test1() {
		System.out.println("This is test 1");
	}
	
	@Test()
	public void test2() {
		System.out.println("This is test 2");
	}
	
	@Test()
	public void test3() {
		System.out.println("This is test 3");
	}
	

}
